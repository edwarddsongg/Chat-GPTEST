{
  "name": "chatgptest",
  "displayName": "chat-gptest",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chatgptest.helloWorld",
        "title": "Hello World"
      },
