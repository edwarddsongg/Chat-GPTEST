{
	"name": "chatgptest",
	"displayName": "chatgptest",
	"description": "chatgptest",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
      {
        "command": "chatgptest.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "chatgptest.test",
        "title": "Test command"
      },
      {
        "command": "chatgptest.activeEditor",
        "title": "Get active editor"
      }
